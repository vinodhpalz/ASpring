<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">

    <title>Hello, world!</title>
  </head>
  <body>
   
	<form>
	<div class="form-group">
	<label for="exampleEmail">Email Address</label>
	<input type="email" class="form-control" id="exampleEmail" placeholder="Enter Email">
	</div>
	
	<div class="form-group">
	<label for="examplePass">Password</label>
	<input type="password" class="form-control" id="examplePass" placeholder="Enter Password">
	</div>
	
	<div class="form-check">
	<input type="checkbox" class="form-check-input" id="dropdownchk">
	<label class="form-check-label" for="dropdownchk">
	Remember Me
	</label>
	</div>
		<button type="submit" class="btn btn-primary">Log In</button>
	</form>

	<br><br>
	
	<form>
	<div class="row">
		<div class="col">
			<input type="text" class="form-control" placeholder="FirstName">
		</div>
		<div class="col">
			<input type="text" class="form-control" placeholder="LastName">	
		</div>
	</div>
	</form>
	
	


